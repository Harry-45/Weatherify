# Weatherify Environment Variables
# Copy this file to `api.env` and fill in your actual values

# Flask secret key for session management (generate a long random string)
FLASK_SECRET=your_secret_key_here_change_this

# OpenWeather API key (get from https://openweathermap.org/api)
WEATHER_API_KEY=your_openweather_api_key

# Spotify API credentials (get from https://developer.spotify.com/dashboard)
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret
SPOTIFY_REDIRECT_URI=http://localhost:5000/spotify_callback
